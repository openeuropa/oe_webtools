<?php

/**
 * Hook implementations for the OpenEuropa Webtools Maps module.
 */

declare(strict_types = 1);

/**
 * Implements hook_theme().
 */
function oe_webtools_maps_theme($existing, $type, $theme, $path) {
  return [
    'oe_webtools_maps_map' => [
      'variables' => [
        'zoom_level' => NULL,
        'longitude' => NULL,
        'latitude' => NULL
      ],
    ],
  ];
}
