{
    "name": "openeuropa/oe_webtools",
    "description": "OpenEuropa Drupal module template.",
    "type": "drupal-module",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1"
    },
  "require-dev": {
    "cweagans/composer-patches": "~1.0",
    "composer/installers": "^1.2",
    "openeuropa/code-review": "^0.2",
    "openeuropa/task-runner": "^0.5",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/console": "^1.6",
    "drupal/config_devel": "~1.2",
    "drush/drush": "^9",
    "drupal/drupal-extension": "^3.4",
    "guzzlehttp/guzzle": "^6.0@dev",
    "symfony/dom-crawler": "^3",
    "webflo/drupal-core-require-dev": "^8.5",
    "phpunit/phpunit": "^4.8.28",
    "nikic/php-parser": "~3"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\oe_webtools\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Drupal\\Tests": "build/core/tests",
      "Drupal\\KernelTests": "build/core/tests",
      "Drupal\\FunctionalTests": "build/core/tests",
      "Drupal\\FunctionalJavascriptTests": "build/core/tests"
    },
    "psr-4": {
      "Drupal\\simpletest\\": "build/core/modules/simpletest/src",
      "Drupal\\Tests\\oe_webtools\\": "tests"
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "https://www.drupal.org/project/drupal/issues/2943172": "https://www.drupal.org/files/issues/2018-04-15/2943172-kernel-test-base-2.patch"
      }
    },
    "installer-paths": {
      "build/core": ["type:drupal-core"],
      "build/profiles/contrib/{$name}": ["type:drupal-profile"],
      "build/modules/contrib/{$name}": ["type:drupal-module"],
      "build/themes/contrib/{$name}": ["type:drupal-theme"]
    }
  }
}
